{
        "manifest_version": 2,
        "name": "Pokemon Safari",
        "version": "1.6.1",
        "description": "Makes wild Pokemon appear in your browser. Find them, capture them, and store them into your Pokedex.",
        "permissions": ["<all_urls>", "storage", "alarms", "notifications"],
        "browser_action": {
            "default_icon": "/images/iconPokeball.png",
            "default_popup": "/html/menu.html"
        },
        "background": {
            "scripts": ["/js/encounter.js"],
            "persistent": false
        },
        "options_page": "/html/options.html",
        "icons": {
            "128": "/images/safari128.png",
            "48": "/images/iconPokeball.png"
        }
}